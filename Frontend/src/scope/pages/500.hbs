@font-face {
	font-family: 'Hargreaves';
	font-display: fallback;
	src:
		url("/fonts/Hargreaves-Regular.woff2") format("woff2"),
		url("/fonts/Hargreaves-Regular.woff") format("woff");
}

@font-face {
	font-family: 'Hargreaves';
	font-weight: 500;
	font-display: fallback;
	src:
		url("/fonts/Hargreaves-Bold.woff2") format("woff2"),
		url("/fonts/Hargreaves-Bold.woff") format("woff");
}

html {
	font-size: 1.125em;
}

body {
	background: #f6f3fa;
	color: #340458;
	display: flex;
	min-height: 100vh;
	flex-direction: column;
	font-family: 'Hargreaves', sans-serif;
	justify-content: center;
}

.wrapper {
	margin: 0 auto;
	max-width: 60ch;
	width: 80%;
}

.rte p {
	color: #444;
	line-height: 1.666666em;
}

.rte h1 {
	font-size: 72px;
	font-weight: 400;
	margin-top: 0;
	text-align: center;
}

.rte h1,
.rte h2 {
	color: #340458;
}

.rte h2 {
	margin-top: 0;
	font-size: 1.66667em;
	margin-bottom: 0.33333em;
	padding-bottom: 0.42857em;
	position: relative;
}

.rte .underline::after {
	background: #fed636;
	bottom: 0;
	content: "";
	display: block;
	height: 4px;
	left: 0;
	position: absolute;
	width: 20px;
}

.rte a {
	color: #59029f;
	font-weight: 500;
	text-decoration: underline;
}

.rte .btn {
	border: 0;
	border-radius: 1.5em;
	cursor: pointer;
	display: inline-block;
	font-weight: 500;
	font-size: 1em;
	line-height: 1.666666em;
	padding: calc((2.5em - 1.6666666em) / 2) 1.111111111em;
	padding-left: 3.33333em;
	position: relative;
	background: #340458;
	color: white;
	text-decoration: none;
}

.rte .btn::before {
	content: "";
	background-image: url(/imgs/arrow-link-white.svg);
	background-repeat: no-repeat;
	background-position: center;
	height: 1.66667em;
	display: inline-block;
	position: absolute;
	left: 1em;
	transform: rotate(180deg);
	vertical-align: top;
	width: 1em;
}

.footer {
	background: #59029f;
	bottom: 0;
	height: 10px;
	left: 0;
	position: fixed;
	width: 100%;
}

#logo {
	display: block;
	height: 30px;
	margin: 3em auto 0;
}

@media screen and (min-width: 900px) {
	.rte h1 {
		font-size: 200px;
		margin-bottom: 0;
	}
}
