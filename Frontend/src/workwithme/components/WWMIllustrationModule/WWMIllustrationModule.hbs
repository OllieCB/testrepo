<div class="illustration js-illustration">
	<div class="container-fluid">
		<div class="heading-block text-center m-b-lg">
			<div class="heading-block__title h5">{{title}}</div>
			<div class="heading-block__sub-title h3">{{{subTitle}}}</div>
		</div>

		<div class="row justify-content-center">
			<div class="illustration--container col-12 col-sm-10 col-md-8">
				<div class="shape shape__default shape__illustration"></div>
				<img src="{{image.large}}" alt="{{image.alt}}" class="img" data-imgs="{{image.small}},{{image.medium}},{{image.large}}">
			</div>
		</div>

		{{#fetchData 'WWMLogoGridModule/data/illustration'}}
			{{> WWMLogoGridModule/WWMLogoGridModule}}
		{{/fetchData}}
	</div>
</div>
